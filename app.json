{
  "expo": {
    "name": "The Daily Answer",
    "slug": "The-Daily-Answer",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/png/Logo-Daily-Answer.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/shared/assets/images/png/bam-splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "./src/shared/assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bibleapp.bamobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/png/Logo-Daily-Answer.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bibleapp.bamobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/shared/assets/images/jpeg/png/Logo-Daily-Answer.png"
    },
    "extra": {
      "eas": {
        "projectId": "51b856be-256f-4672-b9c2-bef53b3286e5"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Bible App to access your camera."
        }
      ]
    ]
  }
}
